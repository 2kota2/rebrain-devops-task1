    git checkout -b feature
    vim last.py
    git add last.py
    git commit -m "Added some decorated elements to last.py"

    git checkout develop
    python last.py > message.txt
    git add message.txt
    git commit -m "Added result message from last.py"

    git remote add origin git@gitlab.rebrainme.com:devops_users_repos/1168/my_last.git
    git push -u origin --all

